{ "version": 2,
"builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
      
    }
  ], 
  "rewrites": 
  [
    { 
      "source": "/(.*)", 
      "destination": "/server.js" ,
      "methods": ["GET", "POST", "PATCH","DELETE","PUT","OPTIONS"],
      "headers":
      {
        "Access-Control-Allow-Origin" : "https://ecommerce-react-frontend-sage.vercel.app"
      }
    }
  ] 
}